{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:webview:allow-set-webview-zoom",
    "dialog:default",
    "opener:default",
    "fs:read-all",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "https://**"
        }
      ]
    }
  ]
}